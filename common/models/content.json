{
  "name": "Content",
  "plural": "contents",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "mainContent": {
      "type": "String"
    },
    "coordinates": {
      "type": "Geopoint"
    }
  },
  "validations": [],
  "relations": {

    "client": {
      "type": "referencesMany",
      "model": "Client",
      "foreignKey": "clientIds",
      "options": {
        "validate": true,
        "forceId": false
      }
    }



  },
  "acls": [{
    "accessType": "*",
    "principalType": "ROLE",
    "principalId": "$everyone",
    "permission": "DENY"
  },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "client",
      "permission": "ALLOW"
    }],
  "methods": {}
}
